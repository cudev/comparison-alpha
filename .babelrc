{
  "presets": [
    "es2015",
    "react",
    "stage-3"
  ],
  "plugins": [
    "system-import-transformer",
    "transform-runtime",
    "transform-flow-strip-types"
  ],
  "env": {
    "development": {
      "sourceMaps": "inline"
    },
    "test": {
      "plugins": [
        "istanbul"
      ]
    }
  }
}
